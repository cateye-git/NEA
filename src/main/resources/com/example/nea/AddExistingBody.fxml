         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="posXField" layoutX="14.0" layoutY="452.0" prefHeight="27.0" prefWidth="90.0" promptText="posX" />
   <TextField fx:id="posYField" layoutX="144.0" layoutY="452.0" prefHeight="27.0" prefWidth="90.0" promptText="posY" />
