<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.nea.AddExistingBodyController">
    <ListView fx:id="SelectBody" fixedCellSize="50.0" layoutX="372.0" layoutY="26.0" prefHeight="613.0" prefWidth="617.0" />
    <Button layoutX="14.0" layoutY="150.0" mnemonicParsing="false" onAction="#addNew" prefHeight="90.0" prefWidth="350.0" text="Add new" />
    <Button layoutX="14.0" layoutY="325.0" mnemonicParsing="false" onAction="#addExisting" prefHeight="90.0" prefWidth="350.0" text="Add selected" />
    <Label layoutX="75.0" layoutY="64.0" prefHeight="62.0" prefWidth="227.0" text="Body Adder" textFill="WHITE">
        <font>
            <Font size="40.0" />
        </font>
    </Label>
   <Label layoutX="14.0" layoutY="415.0" prefHeight="27.0" prefWidth="227.0" text="At pos:" textFill="WHITE">
