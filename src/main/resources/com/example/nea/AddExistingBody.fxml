   <Label layoutX="14.0" layoutY="479.0" prefHeight="27.0" prefWidth="227.0" text="and initial vel:" textFill="WHITE">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="velXField" layoutX="14.0" layoutY="506.0" prefHeight="27.0" prefWidth="90.0" promptText="velX" />
   <TextField fx:id="velYField" layoutX="144.0" layoutY="506.0" prefHeight="27.0" prefWidth="90.0" promptText="velZ" />
   <TextField fx:id="velZField" layoutX="274.0" layoutY="506.0" prefHeight="27.0" prefWidth="90.0" promptText="velZ" />

</AnchorPane>
